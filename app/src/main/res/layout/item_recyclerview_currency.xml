<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="5dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <TextView
        android:id="@+id/textv_code_currency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="@string/tool_codigo_currency"
        android:layout_margin="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
        android:id="@+id/textv_name_currency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="@string/tool_name_currency"
        android:layout_marginStart="@dimen/distance_edittext"
        app:layout_constraintStart_toEndOf="@id/textv_code_currency"
        app:layout_constraintTop_toTopOf="@id/textv_code_currency"
        app:layout_constraintBottom_toBottomOf="@id/textv_code_currency"/>
</androidx.constraintlayout.widget.ConstraintLayout>